<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--允许过度-->
        <item name="android:windowContentTransitions">true</item>
        <!-- Customize your theme here. -->
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="item_menu">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="style_bottom_menu_image">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:state_pressed">false</item>
        <item name="android:checkable">false</item>
    </style>

    <style name="style_bottom_menu_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:state_pressed">false</item>
        <item name="android:checkable">false</item>
    </style>

    <style name="item_nine_image">
        <item name="android:layout_width">@dimen/itemNineImage</item>
        <item name="android:layout_height">@dimen/itemNineImage</item>
        <item name="android:transitionName">@{itemImageUrl}</item>
    </style>

    <style name="item_home_nine_layout">
        <item name="android:layout_width">@dimen/itemNineLayout</item>
        <item name="android:layout_height">@dimen/itemNineLayout</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="messageText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#878DA8</item>
    </style>

    <style name="acconutDetails">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#FF878DA8</item>
    </style>

    <style name="accountMoney">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:textColor">#ffff</item>
        <item name="android:textSize">19dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="registerTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">28dp</item>
        <item name="android:layout_marginTop">24dp</item>
    </style>

    <style name="registerSubTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/dimens_20dp</item>
        <item name="android:layout_marginStart">28dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="loginTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">28dp</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <style name="style_my_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@color/myitem</item>
    </style>

    <style name="style_my_item_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/dimens_15dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="style_my_item_setpsd_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:textColor">#5D5C71</item>
        <item name="android:textSize">14dp</item>
    </style>
    <style name="style_user_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/dimens_15dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="style_inviation_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#878DA8</item>
    </style>

    <style name="style_item_people">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#878DA8</item>
    </style>

    <style name="style_item_num">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="style_set_user_psd_ed">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">@dimen/dimens_5dp</item>
        <item name="android:textColorHint">@color/textsubclor</item>
        <item name="android:textSize">14dp</item>
    </style>
</resources>
